  test -z "$ac_cv_path_mpich_bin" && ac_cv_path_mpich_bin="mpirun"
  ;;
esac
fi
mpich_bin=$ac_cv_path_mpich_bin
if test -n "$mpich_bin"; then
  { $as_echo "$as_me:$LINENO: result: $mpich_bin" >&5
$as_echo "$mpich_bin" >&6; }
else
  { $as_echo "$as_me:$LINENO: result: no" >&5
$as_echo "no" >&6; }
fi


	fi

		$mpich_bin -h > conftmp.log 2>&1
	if ! grep mpich conftmp.log >/dev/null 2>&1; then
		{ $as_echo "$as_me:$LINENO: mpirun binary does not belong to MPICH or was not found,
  if you want to use MPICH, please add specify the location of mpirun using --with-mpicommand" >&5
$as_echo "$as_me: mpirun binary does not belong to MPICH or was not found,
  if you want to use MPICH, please add specify the location of mpirun using --with-mpicommand" >&6;}
		mpich_found=no
	fi
	rm -f conftmp.log
	if test $mpich_found = yes; then
		CPPFLAGS="$mpich_cppflags"
		LDFLAGS=$mpich_ldflags
		LIBS=$mpich_libs
		MPI_INVOCATION="$mpich_bin -np \$NP \$ESPRESSO_CALL"
		{ $as_echo "$as_me:$LINENO: found a working MPICH environment" >&5
$as_echo "$as_me: found a working MPICH environment" >&6;}
	fi

			if test $mpich_found != yes; then { { $as_echo "$as_me:$LINENO: error: MPICH not working" >&5
$as_echo "$as_me: error: MPICH not working" >&2;}
   { (exit 1); exit 1; }; }; fi
			;;
		esac
	fi

		fi
	fi

cat >>confdefs.h <<_ACEOF
#define MPI "$with_mpi"
_ACEOF

	 if test x$with_mpi = xfake; then
  MPI_FAKE_TRUE=
  MPI_FAKE_FALSE='#'
else
  MPI_FAKE_TRUE='#'
  MPI_FAKE_FALSE=
fi




# Test for (obsolete) option --enable-mode
	# Check whether --enable-mode was given.
if test "${enable_mode+set}" = set; then
  enableval=$enable_mode;
		{ $as_echo "$as_me:$LINENO: result: \
********************************************************************************
* The option --enable-mode is obsolete and will be removed in a future version *
* of Espresso! Please use --enable-debug or --enable-profiling instead!        *
********************************************************************************\
		" >&5
$as_echo "\
********************************************************************************
* The option --enable-mode is obsolete and will be removed in a future version *
* of Espresso! Please use --enable-debug or --enable-profiling instead!        *
********************************************************************************\
		" >&6; }
		if test .$enable_mode = .debug; then
		   enable_debug=yes
		fi
		if test .$enable_mode = .profiling; then
		   enable_profiling=yes
		fi

fi


	# test whether to enable debugging
	# Check whether --enable-debug was given.
if test "${enable_debug+set}" = set; then
  enableval=$enable_debug;
else
  enable_debug=no
fi

	if test .$enable_debug = .yes; then

cat >>confdefs.h <<\_ACEOF
#define DEBUG /**/
_ACEOF

	fi

	# test whether to enable profiling
	# Check whether --enable-profiling was given.
if test "${enable_profiling+set}" = set; then
  enableval=$enable_profiling;
else
  enable_profiling=no
fi

	if test .$enable_profiling = .yes; then

cat >>confdefs.h <<\_ACEOF
#define PROFILING /**/
_ACEOF

	fi


	if test .$enable_debug = .yes || test .$enable_profiling = .yes; then
		mdinline=static
	else
		{ $as_echo "$as_me:$LINENO: checking how to inline functions" >&5
$as_echo_n "checking how to inline functions... " >&6; }
		for mdinline in "static inline" "inline static" "inline" "static"; do
		    cat >conftest.$ac_ext <<_ACEOF
/* confdefs.h.  */
_ACEOF
cat confdefs.h >>conftest.$ac_ext
cat >>conftest.$ac_ext <<_ACEOF
/* end confdefs.h.  */

/**** This HAS to be at the beginning of the line for some compilers ****/
#define MDINLINE $mdinline
MDINLINE void test() {}
int
main ()
{
test();
  ;
  return 0;
}
_ACEOF
rm -f conftest.$ac_objext
if { (ac_try="$ac_compile"
case "(($ac_try" in
  *\"* | *\`* | *\\*) ac_try_echo=\$ac_try;;
  *) ac_try_echo=$ac_try;;
esac
eval ac_try_echo="\"\$as_me:$LINENO: $ac_try_echo\""
$as_echo "$ac_try_echo") >&5
  (eval "$ac_compile") 2>conftest.er1
  ac_status=$?
  grep -v '^ *+' conftest.er1 >conftest.err
  rm -f conftest.er1
  cat conftest.err >&5
  $as_echo "$as_me:$LINENO: \$? = $ac_status" >&5
  (exit $ac_status); } && {
	 test -z "$ac_c_werror_flag" ||
	 test ! -s conftest.err
       } && test -s conftest.$ac_objext; then
  works=yes
else
  $as_echo "$as_me: failed program was:" >&5
sed 's/^/| /' conftest.$ac_ext >&5

	works=no
fi

rm -f core conftest.err conftest.$ac_objext conftest.$ac_ext
		    if test .$works = .yes; then break; fi
		done
		if test .$works = .no; then
		   { { $as_echo "$as_me:$LINENO: error: your compiler does not even support \"static\"" >&5
$as_echo "$as_me: error: your compiler does not even support \"static\"" >&2;}
   { (exit 1); exit 1; }; }
		fi
		{ $as_echo "$as_me:$LINENO: result: $mdinline" >&5
$as_echo "$mdinline" >&6; }
	fi

cat >>confdefs.h <<_ACEOF
#define MDINLINE $mdinline
_ACEOF



	# test whether to enable processor optimisation
	# Check whether --enable-processor-optimization was given.
if test "${enable_processor_optimization+set}" = set; then
  enableval=$enable_processor_optimization;
else
  enable_processor_optimization=yes
fi

	if test .$enable_processor_optimization = .yes; then



# Check whether --with-compilertype was given.
if test "${with_compilertype+set}" = set; then
  withval=$with_compilertype;  test .$with_compilertype = .no && with_compilertype=unknown
else
   with_compilertype=unknown
fi

	{ $as_echo "$as_me:$LINENO: checking the compiler type" >&5
$as_echo_n "checking the compiler type... " >&6; }

	if $CC 2>&1 | grep "xlc" >/dev/null 2>&1; then
		with_compilertype=xlc
	fi

	if test .$with_compilertype = .unknown; then

	if $CC -V 2>&1 | grep "Compaq C" >/dev/null 2>&1; then
		with_compilertype=dec
	fi
	if $CC -V 2>&1 | grep "Digital UNIX Compiler" >/dev/null 2>&1; then
		with_compilertype=dec
	fi

		if test .$with_compilertype = .unknown; then

	if $CC -V 2>&1 | grep "Intel" >/dev/null 2>&1; then
		with_compilertype=icc
	fi

			if test .$with_compilertype = .unknown; then

	if $CC -v 2>&1 | grep "gcc" >/dev/null 2>&1; then
		with_compilertype=gcc
	fi

			fi
		fi
	fi
	{ $as_echo "$as_me:$LINENO: result: $with_compilertype" >&5
$as_echo "$with_compilertype" >&6; }

	case $with_compilertype in
	gcc)
	case $target_cpu in
	i*86)		type=ia; m=32; arch=$target_cpu ;;
	Pentium_III)	type=ia; m=32; arch=pentium3; sse=yes ;;
	Atom)		type=ia; m=32; arch=pentiumpro; sse2=yes ;;
	Celeron)	type=ia; m=32; arch=pentium4; sse2=yes ;;
	Pentium_4)	type=ia; m=32; arch=pentium4; sse2=yes ;;
	Pentium_M)	type=ia; m=32; arch=pentium-m; sse2=yes ;;
	Xeon)		type=ia; m=32; arch=pentium4; sse2=yes ;;
	Pentium_4_64)	type=ia; m=64; arch=nocona; sse2=yes ;;
	Xeon_64)	type=ia; m=64; arch=noconamo; sse2=yes ;;
	Athlon)		type=ia; m=32; arch=athlon ;;
	Athlon_MP)	type=ia; m=32; arch=athlon-mp; sse=yes ;;
	Athlon_XP)	type=ia; m=32; arch=athlon-xp; sse=yes ;;
	Opteron)	type=ia; m=64; arch=opteron; sse2=yes ;;
	Athlon_64)	type=ia; m=64; arch=athlon64; sse2=yes;;
	x86_64)		type=ia; m=64; arch=k8 ;;
	Power)		type=pwr; cpu=power ;;
	Power2)		type=pwr; cpu=power2 ;;
	Power*)		type=pwr; cpu=powerpc ;;
	ppc7450)	type=pwr; cpu=7450; altivec=yes ;;
	ppc*)		type=pwr; cpu=powerpc; altivec=yes ;;
	EV67*)		type=alpha; cpu=ev67 ;;
	EV6*)		type=alpha; cpu=ev6 ;;
	EV56*)		type=alpha; cpu=ev56 ;;
	EV5*)		type=alpha; cpu=ev5 ;;
	EV4*)		type=alpha; cpu=ev4 ;;
	sparcv7)	type=sparc; cpu=v7 ;;
	sparcv8)	type=sparc; cpu=v8 ;;
	sparcv9)	type=sparc; cpu=v9 ;;
	sparclite)	type=sparc; cpu=sparclite ;;
	sparclet)	type=sparc; cpu=sparclet ;;
	*)	{ $as_echo "$as_me:$LINENO: WARNING: could not recognize your cpu type, relying on generic optimization" >&5
$as_echo "$as_me: WARNING: could not recognize your cpu type, relying on generic optimization" >&2;}
	esac
	# first try wether the compiler accepts -fast (Apple)

	{ $as_echo "$as_me:$LINENO: checking whether the compiler accepts -fast" >&5
$as_echo_n "checking whether the compiler accepts -fast... " >&6; }
	saved_CFLAGS=$CFLAGS
	CFLAGS="-fast $CFLAGS"
	cat >conftest.$ac_ext <<_ACEOF
