  $as_echo_n "(cached) " >&6
else
  case $poe_bin in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_poe_bin="$poe_bin" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
  for ac_exec_ext in '' $ac_executable_extensions; do
  if { test -f "$as_dir/$ac_word$ac_exec_ext" && $as_test_x "$as_dir/$ac_word$ac_exec_ext"; }; then
    ac_cv_path_poe_bin="$as_dir/$ac_word$ac_exec_ext"
    $as_echo "$as_me:$LINENO: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
done
IFS=$as_save_IFS

  test -z "$ac_cv_path_poe_bin" && ac_cv_path_poe_bin="poe"
  ;;
esac
fi
poe_bin=$ac_cv_path_poe_bin
if test -n "$poe_bin"; then
  { $as_echo "$as_me:$LINENO: result: $poe_bin" >&5
$as_echo "$poe_bin" >&6; }
else
  { $as_echo "$as_me:$LINENO: result: no" >&5
$as_echo "no" >&6; }
fi


		fi
		MPI_INVOCATION="$poe_bin \$ESPRESSO_CALL -procs \$NP"
	fi

			if test $poe_found != yes; then { { $as_echo "$as_me:$LINENO: error: IBM POE MPI not working" >&5
$as_echo "$as_me: error: IBM POE MPI not working" >&2;}
   { (exit 1); exit 1; }; }; fi
			;;
		dmpi)
	saved_libs=$LIBS
	dmpi_found=yes
	{ $as_echo "$as_me:$LINENO: checking whether the $CC command works for MPI" >&5
$as_echo_n "checking whether the $CC command works for MPI... " >&6; }
	dmpi_libs="$LIBS -lmpi"
