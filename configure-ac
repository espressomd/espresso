				if test -f $hdrprf/include/lam.h; then lam_hdr_prf=$hdrprf; lam_hdr=$hdrprf/include; break; fi
			done
			if test $lam_hdr. = .; then
				{ $as_echo "$as_me:$LINENO: did not find lam.h, if you want to use LAM, please specify its location via CPPFLAGS" >&5
$as_echo "$as_me: did not find lam.h, if you want to use LAM, please specify its location via CPPFLAGS" >&6;}
				lam_found=no
			fi
			for libprf in /opt/lam /usr/lam /usr/local/lam /usr /usr/local; do
				for lib in $libprf/lib64 $libprf/lib; do
					if test -f $lib/liblam.a; then lam_lib_prf=$libprf; lam_lib=$lib; break; fi
				done
				if test .$lam_lib != .; then break; fi
			done
			if test $lam_lib. = .; then
				{ $as_echo "$as_me:$LINENO: did not find lam library, if you want to use LAM, please specify its location and name via LDFLAGS" >&5
$as_echo "$as_me: did not find lam library, if you want to use LAM, please specify its location and name via LDFLAGS" >&6;}
				lam_found=no
			fi
			if test $lam_found = yes; then
				lam_cppflags="$CPPFLAGS -I$lam_hdr"
				lam_ldflags="$LDFLAGS -L$lam_lib"
