	icc)
	# check for svml for the vectorizer
	{ $as_echo "$as_me:$LINENO: checking whether we have libsvml" >&5
$as_echo_n "checking whether we have libsvml... " >&6; }
	save_LIBS=$LIBS
	svml_found=no
	LIBS="$LIBS -lsvml"
	# we just try to link, because the functions are not well documented
