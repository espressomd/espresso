install-execpkglibPROGRAMS: $(execpkglib_PROGRAMS)
	@$(NORMAL_INSTALL)
	test -z "$(execpkglibdir)" || $(mkdir_p) "$(DESTDIR)$(execpkglibdir)"
	@list='$(execpkglib_PROGRAMS)'; for p in $$list; do \
	  p1=`echo $$p|sed 's/$(EXEEXT)$$//'`; \
	  if test -f $$p \
	  ; then \
	    f=`echo "$$p1" | sed 's,^.*/,,;$(transform);s/$$/$(EXEEXT)/'`; \
	   echo " $(INSTALL_PROGRAM_ENV) $(execpkglibPROGRAMS_INSTALL) '$$p' '$(DESTDIR)$(execpkglibdir)/$$f'"; \
	   $(INSTALL_PROGRAM_ENV) $(execpkglibPROGRAMS_INSTALL) "$$p" "$(DESTDIR)$(execpkglibdir)/$$f" || exit 1; \
	  else :; fi; \
	done

uninstall-execpkglibPROGRAMS:
	@$(NORMAL_UNINSTALL)
	@list='$(execpkglib_PROGRAMS)'; for p in $$list; do \
	  f=`echo "$$p" | sed 's,^.*/,,;s/$(EXEEXT)$$//;$(transform);s/$$/$(EXEEXT)/'`; \
	  echo " rm -f '$(DESTDIR)$(execpkglibdir)/$$f'"; \
	  rm -f "$(DESTDIR)$(execpkglibdir)/$$f"; \
	done

