    # <materials>
    # state : active
    if { 1 } {
      set matsVMD [material list]
      set matsVMDPREFS {Opaque Transparent BrushedMetal Diffuse Ghost Glass1 Glass2 Glass3 Glossy HardPlastic MetallicPastel Steel Translucent Edgy EdgyShiny EdgyGlass Goodsell AOShiny AOChalky AOEdgy BlownGlass GlassBubble RTChrome}
      # deletions
      foreach ele $matsVMD { if { [lsearch $matsVMDPREFS $ele] == -1 } { material delete $ele } }
      # additions
      foreach ele $matsVMDPREFS { if { [lsearch $matsVMD RTChrome] == -1 } { material add $ele } }
      # set/change properties
      set properties {ambient diffuse specular shininess mirror opacity outline outlinewidth transmode}
      foreach ele {
        {Opaque 0.000 0.650 0.500 0.534 0.000 1.000 0.000 0.000 0.000}
        {Transparent 0.000 0.650 0.500 0.534 0.000 0.300 0.000 0.000 0.000}
        {BrushedMetal 0.080 0.390 0.340 0.150 0.000 1.000 0.000 0.000 0.000}
